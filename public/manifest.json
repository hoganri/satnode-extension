{
  "name": "satnode.space",
  "description": "Send Blockstream Satellite transmissions from your browser!",
  "version": "0.1",
  "manifest_version": 3,
	"action": {
		"default_popup": "index.html",
		"default_title": "satnode.space"
	},
  "icons": {
    "128": "satnode-space-icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}